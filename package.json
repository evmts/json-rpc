{
  "name": "@tevm/json-rpc",
  "version": "1.0.0-rc",
  "description": "Type-safe TypeScript and Zig code generated from the official Ethereum JSON-RPC specification",
  "keywords": [
    "ethereum",
    "json-rpc",
    "tevm",
    "typescript",
    "zig",
    "openrpc",
    "type-safe",
    "execution-apis"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tevm/specs.git"
  },
  "license": "CC0-1.0",
  "contributors": [
    "Fucory <x.com/fucory>"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    }
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts",
    "!src/**/*.test.ts"
  ],
  "scripts": {
    "build": "zig build generate",
    "typecheck": "tsc --noEmit || true",
    "test": "bun test",
    "clean": "zig build clean",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@tevm/primitives": "github:evmts/primitives"
  }
}
